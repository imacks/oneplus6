if file_getprop("/tmp/opoverlay.profile","HydrogenDebloating") == "1" then
	ui_print("[i] Debloat HydrogenOS");

	if file_getprop("/tmp/opoverlay.profile","HydrogenVendorApps") == "1" then
		ui_print("  - HydrogeOS vendor apps");
